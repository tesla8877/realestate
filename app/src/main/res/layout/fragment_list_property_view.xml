<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/list_property_view_frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.listProperties.ListPropertyView">

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/list_property_view_refresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/list_property_view_rv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:listitem="@layout/card_view_item" />

    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

</FrameLayout>